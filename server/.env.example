# サーバー設定
PORT=3001
HOST=localhost
API_KEY=your_api_key_here
CORS_ORIGIN=http://localhost:3000
REQUIRE_AUTH=true
LOG_LEVEL=info
HTTP_LOG=true

# API設定
API_BASE_PATH=/api/v1
SWAGGER_PATH=/api-docs

# ブロックチェーン設定
NETWORK_TYPE=testnet  # mainnet, testnet, local

# ブロックチェーンRPCエンドポイント
MAINNET_RPC_URL=https://ethereum.publicnode.com
TESTNET_RPC_URL=https://sepolia.infura.io/v3/your_infura_key
LOCAL_RPC_URL=http://localhost:8545

# コントラクトアドレス（例）
CONTRACT_PROPERTY_TOKEN_ADDRESS=0x1234567890123456789012345678901234567890
CONTRACT_MARKETPLACE_ADDRESS=0x0987654321098765432109876543210987654321
CONTRACT_REGISTRY_ADDRESS=0xabcdef1234567890abcdef1234567890abcdef12